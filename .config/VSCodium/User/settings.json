{
    "breadcrumbs.enabled": true,
    "diffEditor.renderSideBySide": true,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "none",
    "editor.renderControlCharacters": false,
    "workbench.statusBar.feedback.visible": false,
    "explorer.confirmDelete": false,
    "workbench.startupEditor": "newUntitledFile",
    "cmake.configureOnOpen": false,
    "python.jediEnabled": false,
    "code-runner.fileDirectoryAsCwd": true,
    "python.linting.flake8CategorySeverity.F": "Information",
    "python.linting.flake8Enabled": true,
    "python.terminal.executeInFileDir": true,
    "terminal.integrated.rendererType": "dom",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "code-runner.enableAppInsights": false,
    "editor.detectIndentation": false,
    "python.linting.flake8Args": [
        "--ignore=E501,E301,E302"
    ],
    "window.titleBarStyle": "custom",
    "workbench.colorTheme": "Dark (Molokai)",
    "[python]": {},
    "editor.fontFamily": "Hack",
    "gitlens.currentLine.enabled": false,
    "editor.fontSize": 12,
    "editor.multiCursorModifier": "ctrlCmd",
    "githd.blameView.enabled": false,
    "gitlens.advanced.telemetry.enabled": false,
    "gitlens.codeLens.enabled": false,
    "xmlTools.enforcePrettySelfClosingTagOnFormat": true,
    "python.formatting.autopep8Args": [
        "--max-line-length=100"
    ],
    "explorer.autoReveal": false,
    "todo-tree.tree.showScanModeButton": false,
    "todo-tree.highlights.enabled": false,
    "githd.logView.displayExpressStatus": false,
    "githd.editor.disabled": true,
    "gitHistory.showEditorTitleMenuBarIcons": false,
    "gitlens.liveshare.allowGuestAccess": false,
    "gitlens.showWhatsNewAfterUpgrades": false,
    "update.showReleaseNotes": false,
    "gitlens.statusBar.enabled": false,
    "gitlens.menus": {
        "editorGroup": {
            "blame": false,
            "compare": false
        }
    },
    "debug.console.closeOnEnd": true,
    "debug.console.fontSize": 12,
    "debug.showBreakpointsInOverviewRuler": true,
    "debug.toolBarLocation": "docked",
    "window.zoomLevel": 0,
    "files.insertFinalNewline": true,
    "debug.openDebug": "neverOpen",
    "scssFormatter.tabWidth": 4,
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust"
    },
    "update.mode": "manual",
    "debug.allowBreakpointsEverywhere": true,
    "editor.scrollBeyondLastLine": false,
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.clang_format_fallbackStyle": "none",
    "workbench.iconTheme": "material-icon-theme",
    "explorer.confirmDragAndDrop": false
}
